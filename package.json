{
  "name": "grid-navigator",
  "version": "1.0.0prerelease1",
  "description": "no-fuss accessible keyboard navigation handling to a grid component",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "files": [
    "dist/cjs",
    "dist/esm"
  ],
  "scripts": {
    "clean": "rm -rf ./dist/*",
    "lint": "eslint --fix --ext=.ts src",
    "test": "find ./src -name '*spec.ts' -print | xargs ./node_modules/.bin/mocha --require=ts-node/register",
    "tsc": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "prepublishOnly": "yarn tsc"
  },
  "keywords": [
    "grid",
    "accessibility",
    "keyboard",
    "navigation",
    "arrow keys"
  ],
  "author": "Andrew Peterson",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "*",
    "@types/mocha": "*",
    "@types/node": "*",
    "@types/sinon": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "chai": "*",
    "eslint": "*",
    "keyboard-event-to-string": "*",
    "mocha": "*",
    "mocha-unfunk-reporter": "*",
    "sinon": "*",
    "ts-node": "^10.4.0",
    "typescript": "*"
  }
}
